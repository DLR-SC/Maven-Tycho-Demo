{
    "package": {
        "name": "development-builds",
        "repo": "Maven-Tycho-Demo",
        "subject": "philmfischer",
        "desc": "Continuous Integration artifacts of Maven-Tycho-Demo",
        "website_url": "",
        "issue_tracker_url": "https://github.com/DLR-SC/Maven-Tycho-Demo/issues",
        "vcs_url": "https://github.com/DLR-SC/Maven-Tycho-Demo",
        "github_use_tag_release_notes": false,
        "licenses": ["EPL-1.0"],
        "labels": ["Eclipse", "RCP", "Maven", "Tycho"],
        "public_download_numbers": false,
        "public_stats": false
    },

    "version": {
        "name": "development-snapshot",
        "desc": "Artifacts of development builds - These artifacts may be continuously overwritten",
        "gpgSign": false
    },

    "files":
        [
          {
            "includePattern": "deploy/unsecured/(.*)/plain/binary/(.*\..*)",
            "excludePattern": "(.*/bin/.*|.*/zip/.*)",
            "uploadPattern": "$1/plain/binary/$2",
            "matrixParams": {
              "override": 1
            }
          }, {
            "includePattern": "deploy/unsecured/(.*)/plain/features/(.*\..*)",
            "excludePattern": "(.*/bin/.*|.*/zip/.*)",
            "uploadPattern": "$1/plain/features/$2",
            "matrixParams": {
              "override": 1
            }
          }, {
            "includePattern": "deploy/unsecured/(.*)/plain/plugins/(.*\..*)",
            "excludePattern": "(.*/bin/.*|.*/zip/.*)",
            "uploadPattern": "$1/plain/plugins/$2",
            "matrixParams": {
              "override": 1
            }
          }, {
            "includePattern": "deploy/unsecured/(.*)/plain/artifacts.jar",
            "excludePattern": "(.*/bin/.*|.*/zip/.*)",
            "uploadPattern": "$1/plain/artifacts.jar",
             "matrixParams": {
               "override": 1
             }
          }
        ],
    "publish": true
}